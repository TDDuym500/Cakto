    Title = "Blurry Screen",
    Default = false,
    Callback = function(state)
        Buoi = state
    end
})

-- Li√™n t·ª•c √°p d·ª•ng hi·ªáu ·ª©ng
spawn(function()
    while task.wait() do
        if Buoi then
            blur.Size = 30
        else
            blur.Size = 0
        end
    end
end)
local Buoi = false

Screen:AddToggle("BuoiToggle", {
    Title = "White Screen", -- T√™n hi·ªÉn th·ªã trong UI
    Default = false,
    Callback = function(state)
        Buoi = state
    end
})

-- Ch·∫°y ·∫©n ƒë·ªÉ li√™n t·ª•c ki·ªÉm tra tr·∫°ng th√°i
spawn(function()
    while task.wait() do
        game:GetService("RunService"):Set3dRenderingEnabled(not Buoi)
    end
end)

